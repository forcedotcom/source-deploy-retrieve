{
  "name": "@salesforce/source-deploy-retrieve",
  "version": "1.0.9",
  "gusBuild": "48.14.0",
  "description": "JavaScript library to run Salesforce metadata deploys and retrieves",
  "main": "lib/index.js",
  "author": "Salesforce",
  "license": "BSD-3-Clause",
  "homepage": "https://github.com/forcedotcom/source-deploy-retrieve#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/forcedotcom/source-deploy-retrieve.git"
  },
  "bugs": {
    "url": "https://github.com/forcedotcom/source-deploy-retrieve/issues"
  },
  "keywords": [
    "Salesforce",
    "SalesforceDX",
    "metadata",
    "deploy",
    "retrieve"
  ],
  "files": [
    "lib"
  ],
  "engines": {
    "node": ">=10.5.0"
  },
  "dependencies": {
    "@salesforce/core": "2.4.1",
    "gitignore-parser": "^0.0.2",
    "xmldom-sfdx-encoding": "^0.1.29"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "@types/sinon": "^7.5.2",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "@typescript-eslint/parser": "^2.22.0",
    "chai": "^4",
    "eslint": "^6.8.0",
    "eslint-plugin-jsdoc": "^19.1.0",
    "mocha": "^5",
    "nyc": "^14.1.1",
    "prettier": "1.16.4",
    "shelljs": "0.8.3",
    "sinon": "^7.3.1",
    "source-map-support": "^0.5.16",
    "ts-node": "^8.7.0",
    "typescript": "~3.3"
  },
  "scripts": {
    "build": "rm -rf lib && tsc -b",
    "coverage": "nyc mocha",
    "format": "prettier --config ../../.prettierrc --write './**/*.{js,json}'",
    "lint": "eslint -c .eslintrc.json --ext .ts ./src ./test",
    "local:install": "./scripts/localInstall.js install",
    "local:link": "./scripts/localInstall.js link",
    "local:unlink": "./scripts/localInstall.js unlink",
    "test": "mocha",
    "update:registry": "node ./scripts/metadata-registry/update.js"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "sourceMap": true
  }
}
